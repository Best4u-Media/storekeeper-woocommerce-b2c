{
  "name": "storekeeper/storekeeper-woocommerce-b2c",
  "description": "Backoffice connection extension for WooCommerce",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Ian Wijma",
      "email": "ian@storekeeper.nl"
    },
    {
      "name": "Szymon Lukaszczyk",
      "email": "szymon@storekeeper.nl"
    }
  ],
  "autoload": {
    "psr-0": {
      "StoreKeeper": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "StoreKeeper\\ApiWrapperDev": "vendor/storekeeper/api-wrapper/src/",
      "StoreKeeper\\ApiWrapper\\Test": "vendor/storekeeper/api-wrapper/tests/",
      "StoreKeeper\\ApiWrapperDev\\Test": "vendor/storekeeper/api-wrapper/tests/"
    },
    "psr-4": {
      "StoreKeeper\\WooCommerce\\B2C\\UnitTest\\": "tests/unit/",
      "StoreKeeper\\WooCommerce\\B2C\\TestLib\\": "tests/lib/"
    }
  },
  "require": {
    "php": "^7.4|^8.0|^8.1|^8.2",

    "ext-json": "*",
    "ext-mysqli": "*",
    "symfony/process": "^5",
    "erusev/parsedown": "^1.7",
    "monolog/monolog": "^2.0",
    "adbario/php-dot-notation": "^2.2",
    "ext-zip": "*",
    "ext-bcmath": "*",
    "desarrolla2/cache": "^3.0",
    "aura/sqlquery": "^2.7.1",
    "ext-openssl": "*",
    "storekeeper/api-wrapper": "^5.0"
  },
  "config": {
    "platform": {
      "php": "7.4",
      "ext-mysqli": "7.4",
      "ext-filter": "7.4"
    }
  },
  "require-dev": {
    "mockery/mockery": "^1.3",
    "symfony/dotenv": "^5",
    "fzaninotto/faker": "^1.9",
    "phpoffice/phpspreadsheet": "^1.15",
    "friendsofphp/php-cs-fixer": "^2.18",
    "mikey179/vfsstream": "^1.6",
    "johnpbloch/wp-cli-phar": "^2.5"
  }
}
